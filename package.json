{
  "name": "mwbot-ts",
  "version": "0.0.0",
  "description": "A simple yet flexible TypeScript framework for building MediaWiki bots in Node.js.",
  "main": "index.js",
  "scripts": {
    "mwbot": "ts-node src/test/test_mwbot.ts",
    "mwbot-log": "node -e \"const date = new Date().toISOString().replace(/\\.\\d+Z$/, '').replace(/[-T:]/g, ''); require('child_process').execSync(`ts-node src/test/test_mwbot.ts > logs/${date}.txt 2>&1`);\"",
    "Title": "ts-node src/test/test_Title.ts",
    "general": "ts-node src/test/test_general.ts",
    "build": "tsc -p .",
    "docs": "node -e \"require('fs-extra').emptyDir('docs');\" & npx typedoc --options typedoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dr4goniez/mwbot-ts.git"
  },
  "keywords": [
    "MediaWiki"
  ],
  "author": "Dr4goniez",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Dr4goniez/mwbot-ts/issues"
  },
  "homepage": "https://github.com/Dr4goniez/mwbot-ts#readme",
  "dependencies": {
    "axios": "^1.7.9",
    "axios-cookiejar-support": "^5.0.5",
    "form-data": "^4.0.2",
    "oauth-1.0a": "^2.2.6",
    "tough-cookie": "^5.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "fs-extra": "^11.3.0",
    "ts-node": "^10.9.2",
    "ts-xor": "^1.3.0",
    "typedoc": "^0.27.9",
    "typedoc-plugin-missing-exports": "^3.1.0",
    "types-mediawiki": "^1.9.1",
    "typescript": "^5.7.3"
  }
}
